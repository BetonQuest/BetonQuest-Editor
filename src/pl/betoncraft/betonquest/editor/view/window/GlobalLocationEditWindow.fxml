<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml/1" spacing="5"
	fx:controller="pl.betoncraft.betonquest.editor.controller.GlobalLocationEditController">

	<padding>
		<Insets top="10" left="10" right="10" bottom="10" />
	</padding>

	<Label text="%global-location" id="option-label" />

	<ChoiceBox fx:id="objective" maxWidth="9000" />

	<ButtonBar>
		<buttons>
			<Button text="%ok" onAction="#ok" />
			<Button text="%cancel" onAction="#cancel" />
		</buttons>
	</ButtonBar>

</VBox>

